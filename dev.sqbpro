CREATE VIEW GetProductForSale
AS 
SELECT   P.id, P.typeid, P.title, P.pdesc1, P.pdesc2, P.pdesc3, P.pdesc4, P.pdesc5, P.image, P.image2, P.image3,
S.billno, S.typeid, S.price, T.coinfee, T.reservedfee, T.nextday, T.priceup, T.maxprice, S.ownerid, U.fname, U.lname, S.buyerid, S.photoref, S.ischecked , S.created_at, S.updated_at
FROM Products AS P
JOIN product_mains AS S ON P.pid = S.pid
JOIN product_types AS T ON S.typeid = T.typeid
JOIN Members AS U ON S.ownerid = U.id</sql><current_tab id="1"/></tab_sql></sqlb_project>
